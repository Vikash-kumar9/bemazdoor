  <script>
    const portalFrame = document.getElementById('portalFrame');
  
    function reloadIframe() {
      portalFrame.src = "/portal"; // Replace with the actual endpoint or page you want to load
  
      // Wait for the iframe to finish loading, then scroll to the bottom
      portalFrame.addEventListener('load', function () {
        scrollToBottom();
      });
    }
  
    function scrollToBottom() {
      portalFrame.contentWindow.scrollTo(0, portalFrame.contentDocument.body.scrollHeight);
    }
  
    // Initial scroll to bottom
    scrollToBottom();
  
    // Set up an interval to reload the iframe every 5 seconds
    setInterval(reloadIframe, 20000);
  </script>
  
  

